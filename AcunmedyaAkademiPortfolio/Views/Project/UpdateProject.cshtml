@model AcunmedyaAkademiPortfolio.Models.Table_project

@{
    ViewBag.Title = "UpdateProject";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Proje Güncelleme Sayfası</h2>
<<form method="post">
    <label>Proje Id</label>
    @Html.TextBoxFor(x => x.projectld, new { @class = "form-control" })
    <br />
    <label>Proje Başlık</label>
    @Html.TextBoxFor(x => x.projectTitle, new { @class = "form-control" })
    <br />
    <label>Proje Açıklama</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Projeyi Güncelle</button>
</form>

